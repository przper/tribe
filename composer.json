{
    "name": "przper/tribe",
    "description": "Tribe management helper",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Przper\\Tribe\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "przper",
            "email": "avan.radien@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.3",
        "doctrine/dbal": "^4.0",
        "doctrine/migrations": "3.7.2",
        "symfony/yaml": "^7.1",
        "guzzlehttp/guzzle": "^7.8",
        "symfony/framework-bundle": "^7.1",
        "symfony/flex": "^2.4",
        "symfony/dotenv": "^7.1",
        "symfony/http-client": "^7.1",
        "symfony/runtime": "^7.1",
        "ramsey/uuid": "^4.7",
        "twig/twig": "^3.10",
        "twig/extra-bundle": "^3.10"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^11",
        "behat/behat": "^3.19",
        "rector/rector": "^1.2"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
